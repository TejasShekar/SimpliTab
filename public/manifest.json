{
  "manifest_version": 2,
  "name": "SimpliTab",
  "author": "Tejas Shekar",
  "version": "1.0.1",
  "description": "Focus on your main tasks with Time and weather widgets",
  "icons": {
    "16": "/icons/icon-16.png",
    "32": "/icons/icon-32.png",
    "60": "/icons/icon-60.png",
    "72": "/icons/icon-72.png",
    "96": "/icons/icon-96.png"
  },
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}